{
  "name": "Tag Shot",
  "version": "0.1.0",
  "description": "Take screenshot of every tag",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["html2canvas.js","contentScript.js"]
    }
  ],
  "action": {
    "default_title": "Tag shot",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
